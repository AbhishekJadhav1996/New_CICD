pipeline {
    agent any
 tools {
  jdk 'JDK11'
  maven 'Maven'
}

    stages {
        stage('validate') {
            steps {
                echo 'VALIDATE'
                bat 'mvn clean validate'
            }
        }
        stage('Compile') {
            steps {
                echo 'COMPILE'
                bat 'mvn clean compile'
            }
        }
    }
}

//
